{
    "name": "aeqet/module-ucp",
    "description": "UCP (Universal Commerce Protocol) integration module for Magento 2",
    "type": "magento2-module",
    "license": [
        "proprietary"
    ],
    "authors": [
        {
            "name": "Aeqet",
            "email": "petroveugenv@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "magento/framework": "^103.0",
        "magento/module-quote": "^101.2",
        "magento/module-sales": "^103.0",
        "magento/module-webapi": "^100.4",
        "magento/module-catalog": "^104.0",
        "magento/module-store": "^101.1"
    },
    "require-dev": {
        "magento/magento-coding-standard": "^33",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Aeqet\\Ucp\\": ""
        }
    },
    "scripts": {
        "phpcs": "phpcs --standard=Magento2 .",
        "phpcs-fix": "phpcbf --standard=Magento2 .",
        "phpstan": "phpstan analyse"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
